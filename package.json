{
  "name": "go-implementation-gutter",
  "displayName": "Go Implementation Gutter",
  "description": "Adds a clickable CodeLens for 'Go to Implementation' above interface methods in Go files.",
  "version": "0.0.2",
  "publisher": "your-publisher",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.goToImplementation",
        "title": "Go to Implementation",
        "icon": {
          "light": "./media/go-to-implementation.svg",
          "dark": "./media/go-to-implementation.svg"
        }
      },
      {
        "command": "extension.goToInterface",
        "title": "Go to Interface",
        "icon": {
          "light": "./media/go-to-interface.svg",
          "dark": "./media/go-to-interface.svg"
        }
      }
    ],
    "languages": [
      {
        "id": "go",
        "extensions": [
          ".go"
        ],
        "aliases": [
          "Go",
          "go"
        ]
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "typescript": "^4.9.5",
    "vscode": "^1.1.37",
    "vsce": "^2.18.0"
  },
  "files": [
    "dist",
    "media"
  ]
}
